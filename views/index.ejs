<style>

    .red { background-color: rgb(255, 0, 0); }
    .green { background-color: rgb(0, 255, 0); }
    .blue { background-color: rgb(0, 0, 255); }
    td { padding: 20px 30px 20px 30px; }

</style>
<html>
    <head>
        <title>Remote Control LED</title>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.2.4/jquery.min.js"></script>
        <script src="views/javascript/main.js"></script>
    </head>
    <body>
        <table border="1">
            <tr colspan="3">
                <td class="red">Red</td>
                <td class="green">Green</td>
                <td class="blue">Blue</td>
            </tr>
            <tr colspan="3">
                <td><button class="listener" code="01">On</button></td>
                <td><button class="listener" code="11">On</button></td>
                <td><button class="listener" code="21">On</button></td>
            </tr>
            <tr colspan="3">
                <td><button class="listener" code="00">Off</button></td>
                <td><button class="listener" code="10">Off</button></td>
                <td><button class="listener" code="20">Off</button></td>
            </tr>
        </table>
    </body>
</html>
